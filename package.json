{
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna exec --parallel -- babel src --out-dir lib --copy-files",
    "build:watch": "lerna exec --parallel -- babel src --out-dir lib --copy-files --watch",
    "clean": "lerna exec --parallel -- rimraf lib dist",
    "gh-pages:clean": "rimraf _gh-pages && ncp ./docs ./_gh-pages",
    "gh-pages:build": "./node_modules/.bin/webpack --config webpack.config.ghPage.js",
    "gh-pages:publish": "git-directory-deploy --directory _gh-pages",
    "gh-pages": "npm run build && npm run gh-pages:clean && npm run gh-pages:build && npm run gh-pages:publish",
    "lint": "eslint ./packages/*/src/**/*.js",
    "release": "npm run clean && npm run build && lerna publish",
    "release:next": "npm run clean && npm run build && lerna publish --npm-tag next",
    "test": "lerna exec -- jest --coverage",
    "test:watch": "lerna exec -- jest --watch --coverage",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --mode development"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.4.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "canner-ref-id": "^0.1.2",
    "css-loader": "^0.28.10",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "flow-bin": "^0.66.0",
    "git-directory-deploy": "^1.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.0",
    "lerna": "2.11.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.7.2",
    "prettier": "^1.9.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-inspector": "^2.3.0",
    "react-test-renderer": "^16.2.0",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "styled-components": "^3.2.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {
    "antd": "^3.2.3",
    "canner-helpers": "^0.1.9",
    "react-vega": "^4.0.2",
    "vega-lib": "^4.2.0"
  }
}
